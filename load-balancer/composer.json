{
  "name": "sharryy/load-balancer",
  "description": "A simple load balancer with concurrency",
  "type": "library",
  "version": "1.0.0",
  "require-dev": {
    "pestphp/pest": "^2.34"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "authors": [
    {
      "name": "Sharryy",
      "email": "ibneadam388@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require": {
    "symfony/var-dumper": "^6.4",
    "symfony/console": "^6.4",
    "react/event-loop": "^1.5",
    "react/http": "^1.10"
  }
}
